{
  "name": "vue-tools-test",
  "private": true,
  "version": "5.5.0",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/symcode-fi/vue-tools-test.git"
  },
  "scripts": {
    "commit": "cz",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:tools": "tsc -p tsconfig.tools.json",
    "watch:tools": "tsc --watch -p tsconfig.tools.json",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.vue,.json,.js .",
    "lint:fix": "eslint --ext .ts,.vue,.json,.js --fix .",
    "semantic-release": "semantic-release",
    "semantic-release:dry-run": "semantic-release --dry-run --no-ci"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.0",
    "bootstrap-icons": "^1.9.1",
    "bootswatch": "^5.2.0",
    "js-cookie": "^3.0.1",
    "pinia": "^2.0.21",
    "vue": "^3.2.38",
    "vue-i18n": "^9.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@intlify/vite-plugin-vue-i18n": "^6.0.1",
    "@rollup/plugin-yaml": "^3.1.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/flat": "^5.0.2",
    "@types/js-cookie": "^3.0.2",
    "@types/mustache": "^4.2.1",
    "@types/node": "16",
    "@types/yargs": "^17.0.12",
    "@typescript-eslint/eslint-plugin": "5.36.0",
    "@typescript-eslint/parser": "^5.36.0",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.23.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.4.0",
    "fast-glob": "^3.2.11",
    "flat": "^5.0.2",
    "husky": "^8.0.1",
    "joi": "^17.6.0",
    "mustache": "^4.2.0",
    "prettier": "^2.7.1",
    "pug": "^3.0.2",
    "rollup": "^2.78.1",
    "semantic-release": "^19.0.5",
    "typescript": "^4.8.2",
    "vite": "^3.0.9",
    "vue-tsc": "^0.40.4",
    "yaml": "^2.1.1",
    "yargs": "^17.5.1"
  }
}
